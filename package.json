{
  "author": {
    "name": "Sergey Smekalin"
  },
  "publisher": "Smekalin",
  "license": "MIT",
  "name": "vue-attr-sort",
  "displayName": "Vue sort attributes",
  "description": "Sort multiline attributes",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.34.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Smekalin/Vue-Sort-Attributes"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:VueAttrSort.sort"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "VueAttrSort.sort",
        "title": "Vue sort attributes"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "tslint": "^5.12.1",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  },
  "icon": "images/icon.png"
}
